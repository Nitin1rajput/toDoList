<%- include("header") -%> 


    <div class="box" id="heading">
    <h1> <%= kindOfDay %> </h1>
    </div>
    <div class="box">
      <% for(var i=0; i<task.length; i++){ %>
        <div class="item">
         <input type="checkbox">
          <p><%= task[i] %></p>
        </div>
       <% } %> 
       <form action="/" method="post" class="item">
        <input type="text" name="task" placeholder="New Task" autocomplete="off">
        <button type="submit" name="submit">+</button>
      </form>
        
    </div>
  
    
    <%- include("footer") -%> 
    
    
    
    
    
    
    
    
    
    
    
   
  